{
  "name": "@hypernym/nuxt-font",
  "version": "2.0.0",
  "author": "Hypernym Studio",
  "description": "Auto-optimized font loader for Nuxt.",
  "license": "MIT",
  "repository": "hypernym-studio/nuxt-font",
  "homepage": "https://github.com/hypernym-studio/nuxt-font",
  "configKey": "font",
  "type": "module",
  "main": "./dist/module.mjs",
  "types": "./dist/types.d.ts",
  "exports": {
    ".": {
      "import": "./dist/module.mjs",
      "types": "./dist/types.d.ts"
    }
  },
  "files": [
    "dist",
    "!dist/module.cjs"
  ],
  "keywords": [
    "nuxt",
    "nuxt-module",
    "nuxt-composables",
    "nuxt-font-composables",
    "nuxt-font-module",
    "nuxt-font-loader",
    "font-module",
    "font-loader",
    "nuxt-font",
    "font"
  ],
  "scripts": {
    "dev": "nuxt dev playground",
    "dev:prepare": "nuxt-module-build --stub && nuxi prepare playground",
    "build:module": "nuxt-module-build",
    "build:play": "nuxt build playground",
    "generate:play": "nuxt generate playground",
    "preview:play": "nuxt preview playground",
    "prepublishOnly": "npm run build:module",
    "format": "prettier --write .",
    "lint": "eslint .",
    "fix": "eslint --fix ."
  },
  "devDependencies": {
    "@nuxt/module-builder": "^0.3.1",
    "@types/node": "^18.16.5",
    "@typescript-eslint/eslint-plugin": "^5.59.5",
    "@typescript-eslint/parser": "^5.59.5",
    "eslint": "^8.40.0",
    "eslint-config-prettier": "^8.8.0",
    "nuxt": "^3.4.3",
    "prettier": "^2.8.8",
    "typescript": "^5.0.4"
  },
  "publishConfig": {
    "access": "public"
  },
  "eslintConfig": {
    "extends": "./.config/eslint.config.cjs"
  },
  "prettier": "./.config/prettier.config.cjs"
}
